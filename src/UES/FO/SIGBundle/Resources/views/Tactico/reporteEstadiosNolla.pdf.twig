{% extends "SIGBundle::reporteMaestro.pdf.twig" %}
{% block contenido %}
<h2 text-align="center">Estadios de Nolla </h2>
<p text-align="center">Desde <b> {{ util.fecha(fecha_inicio) }} </b> hasta el <b> {{ util.fecha(fecha_fin) }} </b></p>
<br />
<br />
<br />
<br />
<br />
<br />
	<table width="100%" text-align="center" style="border-color: white; font-size:14px;">
		<tr style="background-color:white; border-color: black;  ">
			<td colspan="11" style="border-color: black; "><p text-align="center"><b>Estadios de Nolla que tienen mayor frecuencia de la pieza seleccionada.</b></p></td>
		</tr>
		<tr style="background-color: white; ">
			<td style="border-color: black; "><p text-align="center">Cero</p></td>
			<td style="border-color: black; "><p text-align="center">Uno</p></td>
			<td style="border-color: black; "><p text-align="center">Dos</p></td>
			<td style="border-color: black; "><p text-align="center">Tres</p></td>
			<td style="border-color: black; "><p text-align="center">Cuatro</p></td>
			<td style="border-color: black; "><p text-align="center">Cinco</p></td>
			<td style="border-color: black; "><p text-align="center">Seis</p></td>
			<td style="border-color: black; "><p text-align="center">Siete</p></td>
			<td style="border-color: black; "><p text-align="center">Ocho</p></td>
			<td style="border-color: black; "><p text-align="center">Nueve</p></td>
			<td style="border-color: black; "><p text-align="center">Diez</p></td>
		</tr>
		<tr style="background-color: white;">
			<td style="border-color: black; "><p text-align="center">{{ cero }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ uno }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ dos }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ tres }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ cuatro }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ cinco }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ seis }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ siete }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ ocho }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ nueve }}</p></td>
			<td style="border-color: black; "><p text-align="center">{{ diez }}</p></td>
		</tr>
	</table>
{% endblock %}